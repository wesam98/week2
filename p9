#include <stdio.h>
#include <stdlib.h>

int main()
{
    int num,res=1;


    int fact = 1;

    printf("Please Enter the number : ");
    scanf("%d",&num);

    int i = 1;

    while( i<=num )
    {
        fact = fact * i;
        i++;
    }
    printf("The factorial is : %d ",fact);
    return 0;
}
